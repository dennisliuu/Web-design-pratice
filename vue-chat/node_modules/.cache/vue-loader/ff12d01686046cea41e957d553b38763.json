rn {\n      ref: firebase.database().ref('chatrooms/'),\n      room: { roomName: '' }\n    }\n  },\n  methods: {\n    onSubmit (evt) {\n      evt.preventDefault()\n\n      let newData = this.ref.push()\n      newData.set({\n        roomName: this.room.roomName\n      })\n      router.go(-1)\n      .catch((error) => {\n        alert(\"Error adding document: \", error)\n      });\n    }\n  }\n}\n</script>\n<style>\n  .jumbotron {\n    padding: 2rem;\n  }\n</style>\n"]}]}